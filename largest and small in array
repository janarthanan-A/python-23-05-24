n = int(input("Enter the number of elements: "))
numbers = [0] * n
print("Enter the elements:")
for i in range(n):
    numbers[i] = int(input())
smallest = numbers[0]
largest = numbers[0]
for i in range(1, n):
    if numbers[i] < smallest:
        smallest = numbers[i]
    elif numbers[i] > largest:
        largest = numbers[i]
print("Smallest element: {}".format(smallest))
print("Largest element: {}".format(largest))
