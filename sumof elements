def sum_array(numbers):
sum = 0
    for number in numbers:
        sum += number
    print("The sum of array elements =", sum)
n = int(input("Enter how many values you want to read: "))
numbers = [0] * n
print("Enter the values of a[{}]:".format(range(n)))
for i in range(n):
    numbers[i] = int(input())
sum_array(numbers)
